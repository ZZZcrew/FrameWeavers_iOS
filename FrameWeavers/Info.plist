<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>UIAppFonts</key>
	<array>
		<string>WSQuanXing.ttf</string>
		<string>STKaiti.ttf</string>
	</array>
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
		<string>processing</string>
		<string>fetch</string>
		<string>audio</string>
	</array>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.frameweavers.videoUpload</string>
		<string>com.frameweavers.comicGeneration</string>
	</array>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>frame-api.zeabur.app</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
				<key>NSExceptionMinimumTLSVersion</key>
				<string>TLSv1.2</string>
			</dict>
		</dict>
	</dict>

	<!-- 支持iTunes文件共享，让用户可以在文件管理器中查看本地画册数据 -->
	<key>UIFileSharingEnabled</key>
	<true/>

	<!-- 支持在文件App中显示 -->
	<key>LSSupportsOpeningDocumentsInPlace</key>
	<true/>

</dict>
</plist>
